<Window x:Class="SGEntregas_Ivan_Almudena.Ventanas.Escritorio.SeleccionarCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SGEntregas_Ivan_Almudena.Ventanas.Escritorio"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:SGEntregas_Ivan_Almudena.ViewModel"
            Title="SeleccionarUsuario" Height="150" Width="400" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <vm:CollectionViewModel x:Key="ColeccionVM"/>
    </Window.Resources>
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Width="Auto" VerticalAlignment="Center">
        <TextBlock Text="Usuarios" HorizontalAlignment="Center"/>
        <ComboBox Name="cmbUsuarios" MinWidth="220" Margin="10" SelectionChanged="cmbUsuarios_SelectionChanged"/>
    </StackPanel>
</Window>
